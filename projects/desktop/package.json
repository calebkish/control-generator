{
  "name": "my-electron-app",
  "version": "0.0.5",
  "description": "My electron app",
  "author": "Caleb Kish",
  "license": "ISC",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "electron": "electron",
    "electron-builder": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start:ts": "electron -- --require ts-node/register .",
    "start": "cross-env NODE_ENV='development' electron .",
    "start:watch": "cross-env NODE_ENV='development' nodemon --watch 'src' --ext 'js' --exec 'electron .'",
    "build:unpacked": "electron-builder --dir",
    "dist": "electron-builder --windows --linux",
    "dist:windows": "electron-builder --windows",
    "dist:linux": "electron-builder --linux",
    "publish": "electron-builder --publish=always --windows --linux",
    "publish:dev": "cross-env READ_DOTENV=1 electron-builder --publish=always --windows --linux"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "electron": "^29.1.5",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@hono/node-server": "^1.9.0",
    "@langchain/community": "^0.0.43",
    "electron-updater": "^6.1.8",
    "hono": "^4.1.5",
    "node-llama-cpp": "^2.8.9"
  }
}
