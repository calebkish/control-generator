{
  "name": "control-generator",
  "version": "0.0.29",
  "description": "Control generator",
  "author": "Caleb Kish",
  "license": "ISC",
  "type": "module",
  "main": "tsc-out/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "node-llama-cpp": "node-llama-cpp",
    "drizzle-kit": "drizzle-kit",
    "tsc": "tsc",
    "electron": "electron",
    "electron-builder": "electron-builder",
    "start": "cross-env NODE_ENV='development' nodemon --ext 'ts,js' --watch 'src/main.ts' --exec 'npm run compile-then-run'",
    "compile-then-run": "tsc && electron .",
    "dist": "electron-builder --windows",
    "http:dev": "cross-env NODE_ENV='development' tsx watch src/http-server/index.ts './user-data' '3000' './'",
    "db:generate-migrations": "drizzle-kit generate --config drizzle.config.ts",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/pdfkit": "^0.13.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.4",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.12",
    "@hono/node-server": "^1.9.0",
    "@hono/zod-validator": "^0.2.1",
    "@libsql/client": "^0.6.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.30.6",
    "electron-updater": "^6.1.8",
    "hono": "^4.1.5",
    "node-llama-cpp": "^3.2.0",
    "openai": "^4.68.4",
    "pdfkit": "^0.15.0",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  }
}
